<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean class="autowiring.mode_byType.beans.Account" id="account">
        <property name="id" value="SAV-102"/>
        <property name="balance" value="100000"/>
    </bean>

    <bean class="autowiring.mode_byType.beans.Customer" id="cus1" autowire="byType">
        <property name="name" value="prachi"/>
    </bean>
</beans>